			<GET_AL11_DIR>
				<item> METHOD GET_AL11_DIR.</item>
				<item> DATA:</item>
				<item> ls_entry TYPE cst_rswatch01_alv.</item>
				<item/>
				<item> CLEAR: rv_phisical_dir.</item>
				<item/>
				<item> CALL &apos;C_SAPGPARAM&apos; ID &apos;NAME&apos; FIELD iv_logical_dir</item>
				<item> ID &apos;VALUE&apos; FIELD rv_phisical_dir.</item>
				<item/>
				<item> DATA:</item>
				<item> my_name TYPE c LENGTH 20,</item>
				<item> all_server TYPE string VALUE &apos;all&apos;, &quot;#EC NOTEXT</item>
				<item> lt_user_dir TYPE STANDARD TABLE OF user_dir,</item>
				<item> ls_user_dir TYPE user_dir.</item>
				<item/>
				<item> IF ( rv_phisical_dir IS INITIAL ).</item>
				<item>* get the name of the current server.</item>
				<item> CALL &apos;C_SAPGPARAM&apos; ID &apos;NAME&apos; FIELD &apos;rdisp/myname&apos;</item>
				<item> ID &apos;VALUE&apos; FIELD my_name.</item>
				<item/>
				<item>* get the name and aliases of ALL userdefined directories</item>
				<item> SELECT SINGLE dirname INTO rv_phisical_dir FROM user_dir WHERE svr</item>
				<item> AND ali</item>
				<item> ENDIF.</item>
				<item/>
				<item> ENDMETHOD.</item>
